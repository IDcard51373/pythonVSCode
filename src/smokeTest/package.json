{
  "name": "code-oss-dev-smoke-test",
  "version": "0.1.0",
  "main": "./src/main.js",
  "scripts": {
    "postinstall": "npm run compile",
    "compile": "npm run copy-driver && npm run copy-driver-definition && tsc",
    "watch": "concurrently \"npm run watch-driver\" \"npm run watch-driver-definition\" \"tsc --watch\"",
    "copy-driver": "cpx src/vscode/driver.js out/vscode",
    "watch-driver": "cpx src/vscode/driver.js out/vscode -w",
    "copy-driver-definition": "node tools/copy-driver-definition.js",
    "watch-driver-definition": "watch \"node tools/copy-driver-definition.js\" ../../src/vs/platform/driver/node",
    "mocha": "mocha",
    "test": "cucumber-js features/* -r source-map-support/register -r out/steps/**/*.js --format json:reports/cucumber-report.json --format summary",
    "report": "node out/reports/html.js"
  },
  "devDependencies": {
    "@types/cucumber": "^4.0.4",
    "@types/htmlparser2": "3.7.29",
    "@types/minimist": "^1.2.0",
    "@types/mkdirp": "0.5.1",
    "@types/mocha": "2.2.41",
    "@types/ncp": "2.0.1",
    "@types/node": "8.0.33",
    "@types/rimraf": "2.0.2",
    "@types/webdriverio": "4.6.1",
    "chai": "^4.2.0",
    "concurrently": "^3.5.1",
    "cpx": "^1.5.0",
    "cucumber": "^5.1.0",
    "cucumber-html-reporter": "^4.0.4",
    "electron": "3.1.2",
    "htmlparser2": "^3.9.2",
    "minimist": "^1.2.0",
    "mkdirp": "^0.5.1",
    "mocha": "^5.2.0",
    "mocha-junit-reporter": "^1.17.0",
    "mocha-multi-reporters": "^1.1.7",
    "ncp": "^2.0.0",
    "portastic": "^1.0.1",
    "rimraf": "^2.6.3",
    "source-map-support": "^0.5.10",
    "strip-json-comments": "^2.0.1",
    "tmp": "0.0.33",
    "typescript": "2.9.2",
    "watch": "^1.0.2"
  }
}
